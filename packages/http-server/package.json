{
  "name": "@leanspec/http-server",
  "version": "0.2.18",
  "description": "Rust HTTP server for LeanSpec UI",
  "type": "module",
  "bin": {
    "leanspec-http": "./bin/leanspec-http.js"
  },
  "scripts": {
    "dev": "LEANSPEC_DEBUG=1 nodemon --watch ./binaries --exec 'node ./bin/leanspec-http.js -v'",
    "typecheck": "echo 'No TypeScript source to check'"
  },
  "keywords": [
    "leanspec",
    "http",
    "server",
    "api",
    "rust"
  ],
  "author": "Marvin Zhang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codervisor/lean-spec.git",
    "directory": "packages/http-server"
  },
  "homepage": "https://lean-spec.dev",
  "bugs": {
    "url": "https://github.com/codervisor/lean-spec/issues"
  },
  "files": [
    "bin/",
    "binaries/",
    "README.md",
    "LICENSE"
  ],
  "optionalDependencies": {
    "@leanspec/http-darwin-arm64": "workspace:*",
    "@leanspec/http-darwin-x64": "workspace:*",
    "@leanspec/http-linux-arm64": "workspace:*",
    "@leanspec/http-linux-x64": "workspace:*",
    "@leanspec/http-windows-x64": "workspace:*"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
