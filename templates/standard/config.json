{
  "name": "Standard",
  "description": "Recommended for most projects - solo devs and small teams",
  "config": {
    "template": "standard",
    "specsDir": "specs",
    "structure": {
      "pattern": "{date}/{seq}-{name}/",
      "dateFormat": "YYYYMMDD",
      "sequenceDigits": 3,
      "defaultFile": "README.md"
    },
    "features": {
      "aiAgents": true
    }
  }
}
